{
  "name": "nescrowee",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "tunnel": "ngrok http 8080",
    "preview:tunnel": "bunx --bun vite build && bunx --bun vite preview --port 8080",
    "build": "bunx --bun vite build",
    "build:dev": "bunx --bun vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "contract:build": "cd contract && rustup run 1.86.0 cargo build --target wasm32-unknown-unknown --release && cd ..",
    "contract:test": "cd contract && rustup run 1.86.0 cargo test && cd ..",
    "contract:deploy": "bash scripts/deploy-contract.sh",
    "contract:init": "bash scripts/init-contract.sh",
    "contract:register-tee": "bash scripts/register-tee.sh",
    "webhook:local": "vercel dev --port 8080"
  },
  "dependencies": {
